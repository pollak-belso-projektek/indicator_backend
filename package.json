{
  "name": "indikator_backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon ./index.js",
    "start": "node ./index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate-docs": "node validate-docs.js",
    "build": "node build.js build",
    "build:docker": "node build.js docker",
    "build:prod": "node build.js production",
    "docker:start": "node build.js start production",
    "docker:start:dev": "node build.js start development", 
    "docker:stop": "node build.js stop",
    "start:all": "concurrently \"npm run start:login\" \"npm run start:gateway\" \"npm run start:main\"",
    "start:all:dev": "concurrently \"npm run dev:login\" \"npm run dev:main\" \"npm run dev:gateway\"",
    "start:all:script": "node start-microservices.js",
    "start:login": "cd login_service && npm start",
    "start:gateway": "cd gateway_service && npm start",
    "start:main": "npm start",
    "dev:login": "cd login_service && npm run dev",
    "dev:gateway": "cd gateway_service && npm run dev",
    "dev:main": "npm run dev",
    "microservices": "concurrently --kill-others-on-fail \"npm run start:login\" \"npm run start:gateway\"",
    "microservices:dev": "concurrently --kill-others-on-fail \"npm run dev:login\" \"npm run dev:gateway\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "@quixo3/prisma-session-store": "^3.1.13",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^9.29.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0"
  }
}
